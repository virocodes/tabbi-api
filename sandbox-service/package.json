{
  "name": "sandbox-service",
  "version": "1.0.0",
  "description": "Node.js service for Daytona and OpenCode SDK operations",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --noEmit",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "snapshot:build": "docker build --platform linux/amd64 -t tabbi-sandbox:1.0.0 -f Dockerfile.snapshot .",
    "snapshot:push": "daytona snapshot push tabbi-sandbox:1.0.1 --name tabbi-dev-env-v2 --cpu 2 --memory 4 --disk 10"
  },
  "dependencies": {
    "@daytonaio/sdk": "^0.134.0",
    "@hono/node-server": "^1.13.7",
    "dotenv": "^17.2.3",
    "hono": "^4.6.14",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20"
  }
}
